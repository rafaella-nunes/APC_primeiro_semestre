a = int(0)
multa = float(0)
carros = int(0)
salvar = float(0)

while (carros != 999):
    carros = int((input()))
    if(carros > 2 and carros < 999):
        carros = carros - 2
        a = a +1
        multa = carros * 12.89
        salvar = salvar + multa
    else:
        continue

print('%.2f' % salvar)
print(a)
